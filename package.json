{
  "name": "next-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap",
    "start": "next start",
    "sitemap": "next-sitemap",
    "format": "prettier --write ./src/\"**/*.{js,ts,tsx}\"",
    "lint": "eslint ./src/\"**/*.{js,ts,tsx}\" --fix",
    "type-check": "tsc --pretty --noEmit",
    "test-all": "npm run format && npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@headlessui/react": "1.2.0",
    "algoliasearch": "4.9.1",
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.9",
    "classcat": "5.0.3",
    "date-fns": "2.21.3",
    "github-markdown-css": "4.0.0",
    "highlight.js": "10.7.2",
    "marked": "2.0.3",
    "next": "10.2.0",
    "nookies": "2.5.2",
    "nprogress": "0.2.0",
    "qs": "6.10.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-google-login": "5.2.2",
    "react-instantsearch-core": "6.11.0",
    "react-instantsearch-dom": "6.11.0",
    "recoil": "0.1.3",
    "swr": "0.5.6"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@tailwindcss/line-clamp": "0.2.0",
    "@tailwindcss/postcss7-compat": "2.1.2",
    "@types/algoliasearch": "3.34.11",
    "@types/axios": "0.14.0",
    "@types/cheerio": "0.22.28",
    "@types/date-fns": "2.6.0",
    "@types/highlight.js": "10.1.0",
    "@types/marked": "2.0.2",
    "@types/node": "14.14.45",
    "@types/nookies": "2.0.3",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.5",
    "@types/react-instantsearch-core": "6.10.2",
    "@types/react-instantsearch-dom": "6.10.0",
    "@types/recoil": "0.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.2.2",
    "babel-plugin-inline-react-svg": "2.0.1",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "heroicons-react": "1.4.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-pwa": "5.2.21",
